<?xml version="1.0" ?>
<robot name="robotRTB">

<!-- KINEMATICS (joints) -->
  <joint name="world_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
  </joint>
  <joint name="joint1" type="continuous">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin rpy="0.0 -0.0 0.0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="joint2" type="continuous">
    <parent link="link1"/>
    <child link="link2"/>
    <origin rpy="0.0 -1.5707963267948966 -1.5707963267948966" xyz="0.0 0.0 0.2"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="joint3" type="prismatic">
    <limit effort="10" velocity="1.0" lower="0.0" upper="1.0"/>
    <parent link="link2"/>
    <child link="link3"/>
    <origin rpy="0.0 -0.0 0.0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="joint4" type="continuous">
    <parent link="link3"/>
    <child link="link4"/>
    <origin rpy="3.141592653589793 -0.0 0.5880026035475676" xyz="0.3 0.2 0.0"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="ee_joint" type="fixed">
    <parent link="link4"/>
    <child link="ee_link"/>
    <origin rpy="0.0 -0.0 -0.9827937232473289" xyz="0.05547001962252285 -0.08320502943378433 0.0"/>
  </joint>

<!-- DYNAMICS (links) -->
  <link name="world"/>
  <link name="base_link"/>
  <link name="link1"/>
  <link name="link2"/>
  <link name="link3"/>
  <link name="link4"/>
  <link name="ee_link"/>

</robot>
